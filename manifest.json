{
  "name": "요소 할당 도구",
  "description": "요소 값을 자동으로 할당해줍니다.",
  "version": "1.0.3",
  "manifest_version": 3,
  "action": {
    "default_popup": "main.html"
  },
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "64": "img/icon64.png",
    "128": "img/icon128.png"
  },
  "permissions": [
    "scripting",
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/welcome.js"]
    }
  ]
}